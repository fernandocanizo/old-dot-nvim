{
  "languageserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    }
  },

  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    }
  },
  "languangeserver": {
    "javascript": {
      "command": "javascript-typescript-langserver",
      "args": ["--strict"],
      "filetypes": ["js"]
    }
  }
}
